{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(js|jsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "recurse": true,
    "destination": "public/docs-frontend",  
    "template": "default",
    "readme": "./README.md"
  }
}
